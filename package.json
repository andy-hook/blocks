  "scripts": {
    "build": "npm run build-icons && gatsby build",
    "build-serve": "gatsby build && gatsby serve",
    "start": "npm run build-icons && gatsby develop",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "test": "jest",
    "test-watch": "npm test -- --watch",
    "snapshots": "npm test -- -u",
    "type-check": "tsc --noEmit --forceConsistentCasingInFileNames",
    "lint": "eslint ./src/",
    "all-checks": "npm run type-check && npm run format && npm run lint && npm run test",
    "netlify-build": "npm run all-checks && npm run build",
